{
  "name": "gls-dashboard",
  "description": "greenlight solutions dashboard",
  "version": "1.0.0",
  "author": "Greenlight Solutions <greenlightsolutionsfoundation@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --config config/webpack.dev.js",
    "build": "npm run test && npm run build:only",
    "build:only": "rimraf public && cross-env NODE_ENV=production webpack --progress --hide-modules --config config/webpack.prod.js",
    "build:run": "APP_BASEPATH=/ npm run build:only && ecstatic public",
    "lint": "eslint \"src/**/*.{js,vue}\" \"config/**/*.js\"",
    "test": "npm run lint && npm run test:only",
    "test:only": "jest",
    "prepush": "npm run lint",
    "deploy:getVersion": "node -r dotenv/config node_modules/parse-on-buddy -v && echo 'Now run: yarn run deploy -- -c <version + 1>'",
    "deploy": "npm run build && node -r dotenv/config node_modules/parse-on-buddy"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "moment": "^2.18.1",
    "parse": "^1.9.2",
    "vue": "^2.2.1",
    "vue-material": "^0.7.4",
    "vue-router": "^2.3.0",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.27.3",
    "dotenv": "^4.0.0",
    "ecstatic": "^2.2.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-vue": "^2.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "node-sass": "^4.5.0",
    "parse-on-buddy": "^2.1.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.16.0",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-merge": "^4.1.0"
  }
}
