{
  "name": "gls-dashboard",
  "description": "greenlight solutions dashboard",
  "version": "1.0.0",
  "author": "Greenlight Solutions <greenlightsolutionsfoundation@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --hot --config config/webpack.dev.js",
    "build": "npm run test && npm run build:only",
    "build:only": "rimraf public && cross-env NODE_ENV=production webpack --progress --hide-modules --config config/webpack.prod.js",
    "build:run": "APP_BASEPATH=/ npm run build:only && ecstatic --port 8081 public",
    "lint": "eslint \"src/**/*.{js,vue}\" \"config/**/*.js\"",
    "test": "npm run lint && npm run test:only",
    "test:only": "jest",
    "prepush": "npm run lint",
    "deploy:getVersion": "node -r dotenv/config node_modules/parse-on-buddy -v && echo 'Now run: yarn run deploy -- -c <version + 1>'",
    "deploy": "npm run build && node -r dotenv/config node_modules/parse-on-buddy"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "moment": "^2.20.1",
    "parse": "^1.11.1",
    "vue": "^2.5.16",
    "vue-material": "^1.0.0-beta-8",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.6",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.28.8",
    "dotenv": "^4.0.0",
    "ecstatic": "^2.2.1",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-vue": "^2.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^3.0.6",
    "husky": "^0.13.3",
    "jest": "^23.0.0-alpha.0",
    "node-sass": "^4.5.0",
    "parse-on-buddy": "^2.1.4",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.16.0",
    "vue-loader": "^14.2.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  }
}
